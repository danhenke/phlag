{
    "name": "danhenke/phlag",
    "description": "Phlag is a minimal developer-facing Feature Flag / Remote Config SaaS.",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/danhenke/phlag/issues",
        "source": "https://github.com/danhenke/phlag"
    },
    "authors": [
        {
            "name": "Dan Henke",
            "email": "dan@thinkalike.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "aws/aws-sdk-php": "*",
        "illuminate/database": "^12.32.5",
        "laravel-zero/framework": "^12.0.2"
    },
    "require-dev": {
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2"
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "autoload": {
        "psr-4": {
            "Phlag\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["phlag"]
}
